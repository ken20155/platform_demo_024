@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Service Plans";
}

<h2 class="mb-3">Service Plans Overview</h2>

<div class="row mb-3">
    <div class="col-md-4">
        <input type="text" id="searchBox" class="form-control" placeholder="Search Service Plan ID..." />
    </div>
</div>

<table class="table table-bordered table-hover" id="plansTable">
    <thead class="table-dark">
        <tr>
            <th>ID</th>
            <th>Date of Purchase</th>
            <th># Timesheets</th>
            <th>Total Hours Worked</th>
        </tr>
    </thead>
    <tbody id="tableBody">
    </tbody>
</table>


